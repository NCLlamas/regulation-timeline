{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/episodes.js": {
      "maxDuration": 30
    },
    "api/episodes/refresh.js": {
      "maxDuration": 30
    }
  }
}